<additive-expression> ::=   <multipicative-expression> + <multipicative-expression>
                          | <multipicative-expression> - <multipicative-expression>
                          ;

<multipicative-expression> ::=  <number>
                              | <multipicative-expression> * <multipicative-expression>
                              | <multipicative-expression> / <multipicative-expression>
                              ;

<number> ::=  T_INTLIT
            ;

                    


